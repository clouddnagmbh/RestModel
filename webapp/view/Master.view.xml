<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="at.clouddna.axiostest.controller.Master"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="To-Do's">
		<content>
			<List items="{/posts/1/comments}" growing="true" growingThreshold="15">
				<items>
					<StandardListItem title="ID: {id}" description="{name}" type="Navigation" press="onTodoPress"/>
				</items>
			</List>
		</content>
	</Page>
</mvc:View>